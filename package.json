{
  "name": "typescriptreact",
  "version": "1.0.0",
  "description": "",
  "main": "index.html",
  "scripts": {
    "test": "jest --coverage",
    "start": "npm run start:dev",
    "build": "npm run build:prod",
    "start:dev": "webpack-dev-server --progress --inline --content-base src/client --history-api-fallback",
    "start:prod": "npm run build:prod && node server.js",
    "build:prod": "webpack -p --define process.env.NODE_ENV='\"production\"' --progress --colors",
    "lint": "tslint src/**/*.ts*"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/tests/.*\\.(ts|tsx|js)$",
    "modulePaths": [
      "<rootDir>/node_modules/",
      "<rootDir>/src/"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/**/*.ts",
      "!**/src/**/*.d.ts"
    ],
    "setupTestFrameworkScriptFile": "./setup-jasmine-env.js",
    "unmockedModulePathPatterns": [
      "./node_modules/jasmine-reporters"
    ],
    "testEnvironment": "jsdom"
  },
  "author": "XComponent <dev@xcomponent.com> (http://www.xcomponent.com)",
  "license": "UNLICENSED",
  "dependencies": {
    "@types/es6-shim": "^0.31.32",
    "@types/jest": "^16.0.1",
    "@types/react": "^0.14.55",
    "@types/react-dom": "^0.14.19",
    "deep-freeze": "0.0.1",
    "es6-promisify": "^5.0.0",
    "es6-shim": "^0.35.2",
    "gojs": "^1.7.4",
    "grommet": "^1.1.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.2",
    "react-router-dom": "^4.0.0-beta.8",
    "reactivexcomponent.js": "2.0.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^3.0.0-beta.17",
    "babel-runtime": "^6.20.0",
    "compression": "^1.6.2",
    "css-loader": "^0.26.1",
    "express": "^4.14.0",
    "html-webpack-plugin": "^2.24.1",
    "jasmine-reporters": "^2.2.0",
    "jest": "^17.0.3",
    "jest-cli": "^17.0.3",
    "jest-junit": "https://github.com/palmerj3/jest-junit.git",
    "node-sass": "^4.0.0",
    "sass-loader": "^4.1.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "ts-jest": "^17.0.3",
    "tslint": "^4.1.0",
    "tslint-loader": "^3.3.0",
    "typescript": "^2.1.4",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  }
}
